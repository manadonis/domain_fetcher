# Environment
NODE_ENV=development

# Server Configuration
PORT=5000
FRONTEND_URL=http://localhost:3000

# Database
MONGODB_URI=mongodb://localhost:27017/domain_intelligence
REDIS_URL=redis://localhost:6379

# JWT Secret
JWT_SECRET=your_super_secret_jwt_key_change_this_in_production

# External APIs
RAPIDAPI_KEY=your_rapidapi_key_here
WHOIS_API_KEY=your_whois_api_key
NAMECHEAP_API_KEY=your_namecheap_api_key
GODADDY_API_KEY=your_godaddy_api_key
GODADDY_SECRET=your_godaddy_secret
SEDO_API_KEY=your_sedo_api_key

# SEO Tools APIs
AHREFS_API_TOKEN=your_ahrefs_api_token
SEMRUSH_API_KEY=your_semrush_api_key
MOZ_ACCESS_ID=your_moz_access_id
MOZ_SECRET_KEY=your_moz_secret_key

# Social Media APIs
TWITTER_BEARER_TOKEN=your_twitter_bearer_token
FACEBOOK_ACCESS_TOKEN=your_facebook_access_token
INSTAGRAM_ACCESS_TOKEN=your_instagram_access_token

# Payment Processing
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password

# File Storage (AWS S3)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
S3_BUCKET_NAME=domain-intelligence-storage

# Monitoring & Analytics
MIXPANEL_PROJECT_TOKEN=your_mixpanel_token
GOOGLE_ANALYTICS_ID=your_ga_tracking_id

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000

# Caching
CACHE_TTL=3600
REDIS_TTL=86400

# Security
BCRYPT_ROUNDS=12
SESSION_SECRET=your_session_secret

# Third-party Integrations
WAYBACK_MACHINE_API=https://web.archive.org/cdx/search/cdx
ALEXA_API_KEY=your_alexa_api_key
SIMILARWEB_API_KEY=your_similarweb_api_key