{
  "name": "domain-intelligence-platform",
  "version": "1.0.0",
  "description": "Comprehensive Domain Finding & Analysis Platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "domain-check-py": "cd utils/external-tools && python3 check_domain.py",
    "domain-check-js": "cd utils/external-tools && node domain-checker-example.js",
    "external-tools": "echo 'Available external tools:' && echo '  npm run domain-check-py <domain1> <domain2>' && echo '  npm run domain-check-js'"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^6.10.0",
    "axios": "^1.5.0",
    "node-whois": "^2.1.3",
    "dns2": "^2.1.0",
    "puppeteer": "^21.3.8",
    "cheerio": "^1.0.0-rc.12",
    "csv-writer": "^1.6.0",
    "csv-parser": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "stripe": "^13.6.0",
    "nodemailer": "^6.9.4",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1",
    "redis": "^4.6.8",
    "agenda": "^5.0.0",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "domain",
    "analysis",
    "valuation",
    "seo",
    "marketplace",
    "investment"
  ],
  "author": "Domain Intelligence Platform",
  "license": "MIT"
}